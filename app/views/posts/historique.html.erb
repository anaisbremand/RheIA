<div class="d-flex">

  <%= render "layouts/dashboard" %>
  <div class="container rounded-4 p-5 d-flex flex-column align-items-center">

    <h1>Mon historique de post</h1>

    <div class=" d-flex f-row">

        <div class="grid">
          <% @historique.each_with_index do |post, index| %>
            <%= render "insta_historique", post: post, index: index %>
          <% end %>
        </div>

    </div>

  <div class="text-center mx-5 ">
    <%= link_to "Générer un autre post", new_post_path, class:"button-valider-historique my-3 px-3" %>
    <%= link_to "Voir mes posts sauvegardés", posts_drafts_path, class:"button-annexe-historique "%>
  </div>

  </div>


</div>
